config ARCH_MT8590
        bool "MediaTek MT8590"
        select GENERIC_TIME
        select GENERIC_CLOCKEVENTS
        select ARCH_HAS_CPUFREQ
        select ARM_AMBA
        select CPU_V7
        select HAVE_SMP
        select NEED_MACH_MEMORY_H
        select ARM_GIC
        select HAVE_MTK_IN_HOUSE_TEE_SUPPORT if 0
        select L1C_OPT
#        select VFP_OPT if 0      
        select HAVE_TRUSTONIC_TEE_SUPPORT if 0
        help
          This enable support for MediaTek MT8590.
